extends layout 
block layout-content 

    .wrapper 
        h1 Bit of a Giggle For You
        
        if data.joke
            p 
                strong Joke  
                span #{data.joke}

        if data.setup

            p 
                strong q: 
                span #{data.setup}
            
            p 
                strong a: 
                span #{data.delivery}
        
    a(href="/joke" class="backbtn") <-- Go Back 